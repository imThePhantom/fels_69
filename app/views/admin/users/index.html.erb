<h1><%= t "admin.user_list" %></h1>

<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th><%= t "admin.name" %></th>
      <th><%= t "admin.email" %></th>
      <th><%= t "admin.admin_title" %></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <%= render @users %>
  </tbody>
</table>
<%= will_paginate %>
<%= link_to t("new_user"), new_admin_user_path %>
