<tr>
  <td><%= user.name %></td>
  <td><%= user.email %></td>
  <td><%= user.admin %></td>
  <td><%= link_to t("admin.show"), admin_user_path(user) %></td>
  <td><%= link_to t("admin.edit"), edit_admin_user_path(user) %></td>
  <td><%= link_to t("admin.delete"), admin_user_path(user), method: :delete,
                  data: {confirm: t("admin.destroy_question")} %></td>
</tr>
